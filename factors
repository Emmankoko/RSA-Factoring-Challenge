/* file - a file assumed to contain natural numbers to factor
 * one per line
 */
int main()
{
	int large_number, i = 2, a, b;

	ifstream in(file);
	if(in.good())
	{
		while(in >> large_number)
		{
			while(large_number % i != 0)
			{
				i++;
			}
			a = large_number / i;
